{
  "name": "adcubex",
  "version": "1.0.0",
  "description": "adcubex",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "build:dev": "webpack -d",
    "start": "npm run build && webpack-dev-server --open-page '.'",
    "format": "prettier --single-quote --write 'src/**/*.{js,css}'",
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "webpack --watch"
  },
  "pre-commit": [
    "test",
    "format",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.instinctools.com/oxy/adcubex.git"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "./__coverage__"
  },
  "keywords": [
    "adcubex"
  ],
  "author": "Archik team",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.11.1",
    "jest": "21.2.1",
    "jsdoc": "3.5.5",
    "pre-commit": "1.2.2",
    "prettier": "1.7.4",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.1"
  },
  "dependencies": {
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0"
  }
}
